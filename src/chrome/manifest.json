      "https://*.ebay.pl/*",
      "https://*.ebay.com.sg/*",
      "https://*.ebay.es/*",
      "https://*.ebay.se/*",
      "https://*.ebay.ch/*",
      "https://*.ebay.com.tw/*",
      "https://*.ebay.co.th/*",
